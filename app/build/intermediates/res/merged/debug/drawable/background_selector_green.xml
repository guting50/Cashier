<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_focused="true" android:drawable="@color/graeen_botton"></item>
    <item android:state_checked="true" android:drawable="@color/graeen_botton"></item>
    <item android:state_pressed="true" android:drawable="@color/graeen_botton"></item>

    <shape>
        <solid android:color="@color/white"></solid>
        <corners android:radius="@dimen/dp_5"></corners>
        <stroke
            android:width="0.5dp"
            android:color="@color/view_gray_line"></stroke>
    </shape>
</selector>
