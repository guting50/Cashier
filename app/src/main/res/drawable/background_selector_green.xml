<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_focused="true" android:drawable="@color/graeen_botton"/>
    <item android:state_checked="true" android:drawable="@color/graeen_botton"/>
    <item android:state_pressed="true" android:drawable="@color/graeen_botton"/>

    <shape>
        <solid android:color="@color/white"/>
        <corners android:radius="@dimen/dp_5"/>
        <stroke
            android:width="0.5dp"
            android:color="@color/view_gray_line"/>
    </shape>
</selector>
